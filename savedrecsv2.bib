@article{ WOS:000394199900005,
Author = {Li, Kai and Qiu, Daowen and Li, Lvzhou and Zheng, Shenggen and Rong,
   Zhenbang},
Title = {Application of distributed semi-quantum computing model in phase
   estimation},
