@article{ WOS:000782418600001,
Author = {Xu, Yuguang and Wang, Liwei and Wang, Chaonan and Zhu, Hongfeng},
Title = {Effective Agent Quantum Private Data Query against Malicious Joint
   Attack with Blind Quantum Computing},
Journal = {INTERNATIONAL JOURNAL OF THEORETICAL PHYSICS},
Year = {2022},
Volume = {61},
Number = {4},
Month = {APR 13},
Abstract = {Recently, the existing quantum private data query protocol based on
   quantum oblivious key distribution has realized the protection function
   of Bob and Alice's personal private data. In this paper, we apply blind
   quantum computing to quantum private data query for achieving effective
   quantum private data query scheme against malicious joint attack.
   Firstly, the identity authentication step of the scheme confirms Alice's
   user identity, which avoids external eavesdropping attacks to a certain
   extent. Secondly, the participation of the agent B-1 reduces the quantum
   capabilities of Alice and Bob. The blindness and correctness of blind
   quantum computing ensure the privacy and security of Alice and Bob and
   avoid malicious joint attacks formed by any party of communication and
   agents. Security analysis and efficiency analysis show that the protocol
   can resist both internal and external attacks, and it has a good
   efficiency.},
