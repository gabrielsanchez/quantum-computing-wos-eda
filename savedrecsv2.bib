@article{ WOS:000372363800003,
Author = {Greganti, Chiara and Roehsner, Marie-Christine and Barz, Stefanie and
   Morimae, Tomoyuki and Walther, Philip},
Title = {Demonstration of measurement-only blind quantum computing},
Journal = {NEW JOURNAL OF PHYSICS},
Year = {2016},
Volume = {18},
Month = {JAN 8},
Abstract = {Blind quantum computing allows for secure cloud networks of
   quasi-classical clients and a fully. fledged quantum server. Recently, a
   new protocol has been proposed, which requires a client to perform only
   measurements. We demonstrate a proof-of-principle implementation of this
   measurement-only blind quantum computing, exploiting a photonic setup to
