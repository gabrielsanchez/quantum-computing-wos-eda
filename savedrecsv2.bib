@article{ WOS:001217279000002,
Author = {Chen, Fan and Jiang, Lei and Muller, Hausi and Richerme, Philip and Chu,
   Cheng and Fu, Zhenxiao and Yang, Min},
Title = {NISQ Quantum Computing: A Security-Centric Tutorial and Survey
   {[}Feature]},
Journal = {IEEE CIRCUITS AND SYSTEMS MAGAZINE},
