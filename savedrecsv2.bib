@article{ WOS:000238794900001,
Author = {Chen, Goong and Diao, Zijian and Kim, Jong U. and Neogi, Arup and
   Urtekin, Kerim and Zhang, Zhighang},
Title = {Quantum dot computing gates},
Journal = {INTERNATIONAL JOURNAL OF QUANTUM INFORMATION},
