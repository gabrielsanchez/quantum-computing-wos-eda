@article{ WOS:001092539800001,
Author = {Moreau, Giuliana Siddi and Pisani, Lorenzo and Mameli, Andrea and Podda,
   Carlo and Cao, Giacomo and Prati, Enrico},
Title = {Gravity Data Inversion by Adiabatic Quantum Computing},
