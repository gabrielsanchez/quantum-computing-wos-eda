@incollection{ WOS:000412299800005,
Author = {Yamamoto, Satoru and Nakazawa, Shigeaki and Sugisaki, Kenji and Sato,
   Kazunobu and Toyota, Kazuo and Shiomi, Daisuke and Takeji, Takui},
