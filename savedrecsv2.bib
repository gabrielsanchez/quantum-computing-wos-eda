@inproceedings{ WOS:000285050700029,
Author = {Zhang, Wen-Ran},
Editor = {Donkor, EJ and Pirich, AR and Brandt, HE},
Title = {YinYang Bipolar Quantum Computing and Bitwise Quantum-Digital
   Cryptography},
Booktitle = {QUANTUM INFORMATION AND COMPUTATION VIII},
Series = {Proceedings of SPIE},
Year = {2010},
Volume = {7702},
Note = {Conference on Quantum Information and Computation VIII, Orlando, FL, APR
   08-09, 2010},
Organization = {SPIE},
Abstract = {Based on YinYang bipolar relativity and quantum entanglement, the
   concepts of bipolar quantum computing and bitwise quantum-digital
   cryptography are presented. Bipolar relativity and quantum entanglement
   provide an equilibrium-based unifying theory of quantum gravity in both
   macroscopic and microscopic agent worlds, which leads to the prediction
   that all action-reaction forces are bipolar quantum entanglement in
   nature and gravity is not necessarily limited by the speed of light.
   While it has been shown with simulation in an earlier paper that bipolar
   quantum entanglement logically enables quantum teleportation without
   conventional communication, it is shown in this paper that YinYang
   bipolar complementarity enables bipolar quantum computing and
   quantum-digital compatibility for bitwise encryption. Since the bipolar
   approach forms a logically complete quantum theory, it opens a new
   research direction to quantum teleportation, computing, and cryptography
   aimed at (1) making prime-number-based cryptography and quantum
   factorization algorithm both obsolete and (2) bringing Schrodinger's cat
   out of the box and quantum computing closer to reality.},
