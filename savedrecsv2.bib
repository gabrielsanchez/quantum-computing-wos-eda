@inproceedings{ WOS:001117817800167,
Author = {Liu, Jonathan and Franklin, Diana},
Book-Group-Author = {ACM},
Title = {Introduction to Quantum Computing for Everyone Experience Report},
Booktitle = {PROCEEDINGS OF THE 54TH ACM TECHNICAL SYMPOSIUM ON COMPUTER SCIENCE
