@article{ WOS:000430165600010,
Author = {Hua, Ming and Tao, Ming-Jie and Alsaedi, Ahmed and Hayat, Tasawar and
   Deng, Fu-Guo},
Title = {Universal Distributed Quantum Computing on Superconducting Qutrits with
   Dark Photons},
