@article{ WOS:000775773100001,
Author = {Ayoade, Olawale and Rivas, Pablo and Orduz, Javier},
Title = {Artificial Intelligence Computing at the Quantum Level},
Journal = {DATA},
Year = {2022},
Volume = {7},
Number = {3},
Month = {MAR},
Abstract = {The extraordinary advance in quantum computation leads us to believe
   that, in the not-too-distant future, quantum systems will surpass
   classical systems. Moreover, the field's rapid growth has resulted in
   the development of many critical tools, including programmable machines
   (quantum computers) that execute quantum algorithms and the burgeoning
   field of quantum machine learning, which investigates the possibility of
   faster computation than traditional machine learning. In this paper, we
   provide a thorough examination of quantum computing from the perspective
   of a physicist. The purpose is to give laypeople and scientists a broad
   but in-depth understanding of the area. We also recommend charts that
   summarize the field's diversions to put the whole field into context.},
