@article{ WOS:000454069700005,
Author = {Cristian, Espinel-Lopez and Alvaro, Martinez-Gomez and Marisol,
   Aguilar-Echeverria and Hipatia, Manay-Manay},
Title = {Evolution of quantum computing components and non-destructive quantum
   measurements in modern computing},
