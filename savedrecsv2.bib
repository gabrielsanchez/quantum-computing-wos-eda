@article{ WOS:001179413900001,
Author = {Nguyen, Hoa T. and Usman, Muhammad and Buyya, Rajkumar},
Title = {QFaaS: A serverless function-as-a-service framework for quantum
   computing},
Journal = {INTERNATIONAL JOURNAL OF PHARMACEUTICS},
Year = {2024},
