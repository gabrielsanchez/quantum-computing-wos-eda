@article{ WOS:000915488000001,
Author = {Yang, Ning and Guo, Jing},
Title = {A Quantum-Computing-Based Method for Solving Quantum Confinement Problem
   in Semiconductor},
Journal = {IEEE TRANSACTIONS ON ELECTRON DEVICES},
