@inproceedings{ WOS:001164081800034,
Author = {Wu, Anbang and Ding, Yufei and Li, Ang},
Book-Group-Author = {ACM},
Title = {QuComm: Optimizing Collective Communication for Distributed Quantum
   Computing},
Booktitle = {56TH IEEE/ACM INTERNATIONAL SYMPOSIUM ON MICROARCHITECTURE, MICRO 2023},
Year = {2023},
