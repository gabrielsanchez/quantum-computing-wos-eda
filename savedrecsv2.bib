@article{ WOS:001153780500001,
Author = {Schwetz, M. and Noack, R. M.},
Title = {Three-qubit Deutsch-Jozsa in measurement-based quantum computing},
Journal = {INTERNATIONAL JOURNAL OF QUANTUM INFORMATION},
Year = {2024},
Month = {2024 JAN 27},
Abstract = {Measurement-based quantum computing (MBQC), an alternate paradigm for
   formulating quantum algorithms, can lead to potentially more flexible
   and efficient implementations as well as to theoretical insights on the
